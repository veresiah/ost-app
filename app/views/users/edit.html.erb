<h1>@user.first_name Profile</h1>
  <%= form_for(@user) do |f| %>
    <label>First Name:</label><br>
    <%= f.text_field :first_name %><br>
  
    <label>Last Name:</label><br>
    <%= f.text_field :last_name %><br>

    <label>Username:</label><br>
    <%= f.text_field :username %><br>

    <label>Password:</label><br>
    <%= f.password_field :password_digest %><br>

    <label>Street Address:</label><br>
    <%= f.text_field :street_address %><br>

    <label>City:</label><br>
    <%= f.text_field :city %><br>

    <label>State:</label><br>
    <%= f.text_field :state %><br>

    <label>Zip Code:</label><br>
    <%= f.text_field :zip_code %><br>

    <label>Phone Number:</label><br>
    <%= f.text_field :phone_number %><br>

    <label:</label>Email:</label><br>
    <%= f.text_field :email_address %><br>

    <br><%= f.submit %>
  <% end %> 
